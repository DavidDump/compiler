// Example code for printing hello world using win32 API
STD_HANDLE_STDOUT :: -12;

foo :: (name: u64) -> u64 {
    go := name + 1;
    return 1 + 1;
}

#library "kernel32.dll";
#extern WriteFile    :: (handle: s64, data: string, dataLen: u64, bytesRead: u64*, overlapped: void*);
#extern GetStdHandle :: (handle: s64);
#extern ExitProcess  :: (code: s64);

main :: () -> u8 {
    stdout := GetStdHandle(STD_HANDLE_STDOUT);
    WriteFile(stdout, "Hello world", 11, 0, 0);
    return 0;
}
